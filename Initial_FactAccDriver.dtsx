                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
  DriverKey,

  SUM(TotalTripTotalCostAmount) AS RunningTotalTripTotalCostAmount,
  SUM(TotalTripFeeAmount) AS RunningTotalTripFeeAmount,
  SUM(TotalTripNetDriverIncomeAmount) AS RunningTotalTripNetDriverIncomeAmount,

  AVG(AvgTripTotalCostAmount) AS RunningAvgTripTotalCostAmount,
  AVG(AvgTripFeeAmount) AS RunningAvgTripFeeAmount,
  AVG(AvgTripNetDriverIncomeAmount) AS RunningAvgTripNetDriverIncomeAmount,

  SUM(TotalTripDistanceInKm) AS RunningTotalTripDistanceInKm,
  SUM(TotalTripDurationInMin) AS RunningTotalTripDurationInMin,
  SUM(TotalTripStopDurationInMin) AS RunningTotalTripStopDurationInMin,

  AVG(AvgTripDistanceInKm) AS RunningAvgTripDistanceInKm,
  AVG(AvgTripDurationInMin) AS RunningAvgTripDurationInMin,
  AVG(AvgTripStopDurationInMin) AS RunningAvgTripStopDurationInMin,

  AVG(AvgDriverArrivalInSec) AS RunningAvgDriverArrivalInSec,
  AVG(AvgPessangerBoardingInSec) AS RunningAvgPessangerBoardingInSec,

  AVG(
      AvgRateByPassenger
  )  AS RunningAvgRate,

  COUNT(AvgRateByPassenger)  AS RunningTotalRateCount,

  SUM(TripCount) AS RunningTotalTripCount

FROM Core.FactDailyDriverTrip
GROUP BY DriverKey</property>
