                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
((re.InitiateRequestTimeKey/10000)%24) as hourkey,
count(*) as TotalRequestCount,

sum(case when re.RequestStatusKey=2 then 1 else 0 end) as MatchedRequsetCount,

CAST(
sum(case when re.RequestStatusKey&lt;&gt;2 then 1 else 0 end)
AS float
) / NULLIF(count(*),0) as CancellationRate,

1 - (
CAST(
sum(case when re.RequestStatusKey&lt;&gt;2 then 1 else 0 end)
AS float
) / NULLIF(count(*),0)
) as MatchRate

from [snapdb-dw].Core.FactTransactionRequest as re
where re.InitiateRequestDateKey&gt;
( SELECT CONVERT(INT, FORMAT(
    (SELECT TOP 1 [LastRunEnd]
     FROM [snapdb-dw].[dbo].[ETL_Metadata]
     WHERE LastRunStatus = 'Success' 
       AND TableName = 'FactAccRequestPerHour'
     ORDER BY LastRunEnd DESC),
    'yyyyMMdd'))
)
group by  ((re.InitiateRequestTimeKey/10000)%24)</property>
